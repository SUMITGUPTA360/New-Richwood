    <fields>
        <fullName>Age_Calculation__c</fullName>
        <externalId>false</externalId>
        <formula>IF((FLOOR((TODAY() - Date_of_Birth_del__c  ) / 365.2425))=0,
 &quot; &quot;, 
 IF((FLOOR((TODAY() -  Date_of_Birth_del__c ) / 365.2425))=1, 
 text(FLOOR((TODAY() -  Date_of_Birth_del__c ) / 365.2425))&amp;&quot;year&quot;, 
 text(FLOOR((TODAY() -  Date_of_Birth_del__c ) / 365.2425))&amp;&quot;years&quot;))
&amp;
IF((FLOOR(MOD((TODAY()- Date_of_Birth_del__c ),365.2425)/30))=0,
 &quot; &quot;, 
 IF((FLOOR(MOD((TODAY()- Date_of_Birth_del__c ),365.2425)/30))=1, 
 text(FLOOR(MOD((TODAY()- Date_of_Birth_del__c ),365.2425)/30))&amp;&quot;month&quot;, 
 text(FLOOR(MOD((TODAY()- Date_of_Birth_del__c ),365.2425)/30))&amp;&quot;months&quot;) )
&amp;
IF((FLOOR(mod(MOD((TODAY()- Date_of_Birth_del__c ),365.25),30)/7))=0,
 &quot; &quot;, 
 IF((FLOOR(mod(MOD((TODAY()- Date_of_Birth_del__c),365.25),30)/7))=1, 
 text(FLOOR(mod(MOD((TODAY()- Date_of_Birth_del__c),365.25),30)/7))&amp;&quot;week&quot;, 
 text(FLOOR(mod(MOD((TODAY()- Date_of_Birth_del__c ),365.25),30)/7))&amp;&quot;weeks&quot;) )
&amp;
IF((FLOOR(mod(mod(MOD((TODAY()-Date_of_Birth_del__c ),365.2425),30.44),7)))=0,
 &quot; &quot;, 
 IF((FLOOR(mod(mod(MOD((TODAY()- Date_of_Birth_del__c),365.2425),30.44),7)))=1, 
 text(FLOOR(mod(mod(MOD((TODAY()- Date_of_Birth_del__c),365.2425),30.44),7)))&amp;&quot;day&quot;, 
 text(FLOOR(mod(mod(MOD((TODAY()- Date_of_Birth_del__c ),365.2425),30.44),7)))&amp;&quot;days&quot;) )</formula>
        <label>Age</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
