<script>
    // CHECK IF WE NEDD THIS VARIABLE
    var isSdcPage = false;
    //isSdcPage = (typeof picassoRuntime !== 'undefined');
    
    var lang = getLang();
    
    function readCookie(name) {
        return (name = new RegExp('(?:^|;\\s*)' + ('' + name).replace(/[-[\]{}()*+?.,\\^$|#\s]/g, '\\$&') + '=([^;]*)').exec(document.cookie)) && name[1];
    }
    if (readCookie('apex__dfc_locale') === null) {
        document.cookie = 'apex__dfc_locale=' + lang.code + ';path=/;';
        window.location.reload()
    }
    // check if we need this function
    if (lang.code !== readCookie('apex__dfc_locale')) {
        document.cookie = 'apex__dfc_locale=' + lang.code + ';path=/;';
    }
</script>
