<script>
    $( document ).ready(function() {
        // Disables the index dropdown if it's not force.com
        $('#select_subject').change(function() {
            if (($(this).val().indexOf('Force.com') != -1) || ($(this).val().indexOf('All Services') != -1)){
            	$('#select_type').prop('disabled',false);
        	} else {
        		$('#select_type').prop('disabled',true);
        	}
        });
        var percent = 0.40;
        var add_width = (percent*window.innerHeight);
        $("#open-console_btn").toggle(function(){
            $("#open-console_btn").text('x');
            $('#api-console').attr('scrolling','yes');
            $('#api-console').animate({height: add_width},200);
        },function(){
            $("#open-console_btn").text('+');
            $('#api-console').attr('scrolling','no');
            $('#api-console').animate({height: 40 },200);
        });
        changeTocIconState();
        highlightToc();
        call_campaigns_cookie();
    });
        function call_campaigns_cookie(){
            var params = document.location.search.substring(1);
        	var utm_source = '';
        	var utm_medium = '';
        	var utm_campaign = '';
        	var dr = '';
        	params_array = params.split('&');
        	for(i=0;i<params_array.length;i++){
        		param_array = params_array[i].split('=');
        		if(param_array[0]=='utm_source'){
        			utm_source = param_array[1];
        		}else if(param_array[0]=='utm_medium'){
        			utm_medium = param_array[1];
        		}else if(param_array[0]=='utm_campaign'){
        			utm_campaign = param_array[1];
        		}else if(param_array[0]=='dr'){
        			dr = param_array[1];
        		}
        	}
        
        	if(utm_source!=null && utm_source!='' && 
        		utm_medium!=null && utm_medium!='' && 
        		utm_campaign!=null && utm_campaign!=''){
        		//do call
        		$.get( "/events/campaigns_cookie", { utm_source: utm_source, utm_medium: utm_medium, utm_campaign: utm_campaign, dr: dr } );
        	}
        }
        function highlightToc(){
            var lastSlashIndex = location.pathname.lastIndexOf("/");
            var lastSlash = lastSlashIndex + 1;
            var pageURL = location.pathname.substr(lastSlash);
            var pageName = pageURL.substr(0, pageURL.lastIndexOf("."));    
            var pageHash = location.hash;
            if(pageHash!=undefined && pageHash!=''){
                var foundLi = $("#toc a[href$='" + pageHash + "']");
                if(foundLi!=undefined && foundLi!='' && foundLi.length>0){
                    $("#toc a[href$='" + pageHash + "']").parent("div").toggleClass('active');
                    $("#toc a[href$='" + pageHash + "']").parents('div.collapse').collapse('show');
                }else{
                    $("#toc a[href$='" + pageHash.substring(1, pageHash.length) + '.htm' + "']").parent("div").toggleClass('active');
                    $("#toc a[href$='" + pageHash.substring(1, pageHash.length) + '.htm' + "']").parents('div.collapse').collapse('show');
                }
            }else{
                if( pageName != undefined && pageName != ''){
                    var foundLi = $("div#" + pageName);
                    if(location.search==undefined || location.search==''){
                        foundLi = $("a[href*='" + pageURL + "']");
                        $("a[href~='" + pageURL + "']").parent("div").toggleClass('active');
                        $("a[href~='" + pageURL + "']").parents('div.collapse').collapse('show');
                    }else{
                        foundLi = $("a[href*='" + pageURL + location.search + "']");
                        if(foundLi!=undefined && foundLi!=''){
                            $("a[href$='" + pageURL + location.search + "']").parent("div").toggleClass('active');
                            $("a[href$='" + pageURL + location.search + "']").parents('div.collapse').collapse('show');
                        }else{
                            $("a[href$='" + pageURL + location.search + pageName + "']").parent("div").toggleClass('active');
                            $("a[href$='" + pageURL + location.search + pageName + "']").parents('div.collapse').collapse('show');
                        }
                    }
                }else{
                    $("#toc .toc-level-1:eq(0), .mobile-toc .toc-level-1:eq(0)").addClass("active");
                }
            }
        }
    </script>
