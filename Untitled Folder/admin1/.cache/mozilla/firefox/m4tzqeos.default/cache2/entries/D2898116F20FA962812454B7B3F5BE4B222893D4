<script>
  function moveRight(ele, x, time) {
    if(x >= ele.parent().width()){
      ele.css('left', -x);
    }
    x = (ele.parent().width());
  
    ele.animate({
      'left': x + 'px'
    },time,"linear",function(){
        moveRight(ele,x,time);
    });
  }
  function moveLeft(ele, x, time) {
    if(x <= -200){
      ele.css('left', ele.parent().width());
    }
    x = -200;
  
    ele.animate({
      'left': x + 'px'
    },time,"linear",function(){
        moveLeft(ele,x,time);
    });
  }
  
  
  
  function resizeContainer(){
  
    var el = $('.trailhead-error-content');
    var c_width = $(el).outerWidth();
    //var ratio = 1.5301435407; // This is the background image proportion
    var ratio = 2.523659306; // This is the background image proportion
    var curentHeight = $(el).height();
    var new_height = Math.floor(c_width / ratio);
  
    var imgResized = false;
    var elStartPosition = Math.floor($('.trailhead-error-content').offset().top);
    var imgHeight = new_height;
    var importantImgPoint_topPercentage = 38;
    var importantImgPoint_bottomPercentage = 67;
    var importantImgPoint_topPosition = Math.floor(imgHeight * importantImgPoint_topPercentage / 100);
    var importantImgPoint_bottomPosition = Math.floor(imgHeight * importantImgPoint_bottomPercentage / 100);
    var topLimit = Math.floor( $('.error-description').offset().top + $('.error-description').outerHeight() );
    var imgTopPos = Math.floor( $(el).height() + $('.trailhead-error').offset().top - new_height );
    //if( c_width > 810 ){
      //$(el).height( (new_height+200) +'px');
      //imgResized = true;
      //$(el).css('padding-top', '300px');
    //}
  
    // Check if body height is higher than window height :)
    if ($("body").height() > $(window).height()) {
      //Vertical Scrollbar
  
      //We are viewing of image
      var imgPixDisplayed = '';
      if( imgResized ){
        imgPixDisplayed = Math.floor( $(window).height() - $('.trailhead-error').offset().top - 200 );
      }else{
        imgPixDisplayed = Math.floor( $(window).height() - $('.trailhead-error').offset().top - curentHeight + imgHeight);
      }
      //var imgPercentageDisplayed = Math.floor( imgPixDisplayed * 100 / imgHeight );
  
      if( imgPixDisplayed < importantImgPoint_bottomPosition && (imgTopPos + importantImgPoint_topPosition) > topLimit ){
        // reduce $(el) height to can see the Trail Closed sign that is down the visibile area
        var diff = Math.abs(importantImgPoint_bottomPosition - imgPixDisplayed);
  
        if( (imgTopPos + importantImgPoint_topPosition - diff) < topLimit ){
          diff = Math.abs(topLimit - (imgTopPos + importantImgPoint_topPosition));
  
        }
  
        //console.info('.... SET 1 TO: '+ ($(el).height() - diff));
        $(el).height( ($(el).height() - diff) +'px');
  
  
      }else if( imgTopPos > topLimit ){
        // Reduce $(el) height to dont have too much space between text and background-image
        var diff = imgTopPos - topLimit;
        //console.info('.... SET 2 TO: '+ ($(el).height() - diff));
        $(el).height( ($(el).height() - diff) +'px');
      }
    }
  
    if( (imgTopPos + importantImgPoint_topPosition) < topLimit ){
      //Increase the $(el) height to dont have the Trail Closed sign behind the text
      var diff = topLimit - (imgTopPos + importantImgPoint_topPosition);
      //console.info('.... SET 3 TO: '+ ($(el).height() + diff));
      $(el).height( ($(el).height() + diff) +'px');
    }
  }
  
  $(document).ready(function(){
    moveRight($('.x1'),0,30000);
    moveRight($('.x2'),0, 25000);
    moveLeft($('.x3'), 10, 31000);
  
    resizeContainer();
  
    $(window).resize(function(){
      resizeContainer();
    });
  });
</script>

