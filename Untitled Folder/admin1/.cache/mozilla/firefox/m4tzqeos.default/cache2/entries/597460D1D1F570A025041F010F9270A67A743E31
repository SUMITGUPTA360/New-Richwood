<script>
  function initializeScrollEvents() {
    $('body').scrollspy();
  
    var options = $('body').data('scrollspy');
    options.offsets = [];
    options.targets.each(function (index, item) {
      options.offsets[index] = $(item).offset().top - 150;
    });
    $('body').data('scrollspy', options);
  
    setupAffixTopicList();
  }
  
  function setupAffixTopicList() {
    var topPos = parseInt($('#topic-list')[0].offsetTop) + 120;
    var bottomPos = $('footer').outerHeight() + $('#topic-list').outerHeight() + 40;
    $(window).scroll(function(){
      affixTopicList(topPos, bottomPos);
    });
  }
  
  function affixTopicList(topPos, bottomPos) {
    var yPos = $(window).scrollTop();
    var docHeight = $(document).outerHeight();
  
    if(yPos > (docHeight-bottomPos-105)) {
      $('#topic-list').removeClass('affix');
      $('#topic-list').addClass('affix-bottom');
    } else {
      $('#topic-list').removeClass('affix-bottom');
      if(yPos > topPos) {
        $('#topic-list').addClass('affix');
      } else {
        $('#topic-list').removeClass('affix');
      }
    }
  }
  
  if (typeof addthis !== 'undefined') {
    addthis.addEventListener('addthis.ready', initializeScrollEvents);
  } else {
    $('document').ready(initializeScrollEvents);
  }
  
  $(function() {
    // trigger scroll event to get window in correct state if
    // refreshing while scrolled down
    $('window').trigger('scroll');
  });
  
  (function() {
    // fix code snippets
    $.each($('pre'), function(index,item) {
      $(item).html($(item).html().replace(/<[^>]+>/gi, ''));
    });
  
    $('pre').addClass('brush: apex;');
    SyntaxHighlighter.config.clipboardSwf = "/assets/clipboard-583bcb178f3e17f27a8042d7eea29f8c9c039b17aa3cbf8bdfa8442ab264191e.swf";
    SyntaxHighlighter.highlight();
  })();
</script>
<script>
  $('.language-social .select-dropdown li').on('click', function(e) {
    if($(this).hasClass('active')) {
      $(this).closest('ul').toggleClass('open');
    } else {
      var newLang = $(this).data('language');
      var langRegExp = /^[a-z]{2}(?:-[A-Z]{2})?(\?.+)?$/;
      var parts = document.location.pathname.split('/');
  
      // if it has more than 1 part and the part on position 1 is a language overwrite it
      // with the new language
      if (parts.length > 1 && langRegExp.test(parts[1])) {
          parts[1] = newLang
      } else { // if it does not have a language add it at position 1
          parts.splice(1, 0, newLang);
      }
      var redirect = parts.join('/');
  
      document.location.replace(redirect);
    }
  });
  
  $('body').on('click', function(e) {
    if($(e.target).parents('.select-dropdown').length == 0) {
      $('.language-social .select-dropdown.open').removeClass('open');
    }
  });
</script>

